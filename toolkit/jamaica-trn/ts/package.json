{
  "name": "jamaica-trn",
  "version": "0.1.0",
  "description": "Jamaica Tax Registration Number (TRN) validator and formatter",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": { "import": "./dist/index.mjs", "require": "./dist/index.js", "types": "./dist/index.d.ts" }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "keywords": ["jamaica", "trn", "tax", "validation"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamaica-digital/jamaica.git",
    "directory": "toolkit/jamaica-trn/ts"
  },
  "homepage": "https://jamaica.digital/packages/trn",
  "bugs": "https://github.com/jamaica-digital/jamaica/issues",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@jamaica-digital/tsconfig": "workspace:*",
    "tsup": "^8.0.0",
    "vitest": "^3.0.0",
    "typescript": "^5.7.0"
  }
}
